{
    "jip":{
        "logging":{
            "logfile":"logs/jip.log",
            "pattern":"[%-5p] [%t] [%d{dd MMM yyyy HH:mm:ss,SSS}] [%c{2}] : %m%n",
            "log4j.rootLogger": "info",
            "log4j.logger.org.reflections": "fatal"
        }
    },
    "jobs":{
        "idservice":{
            "service" : "jip.jobs.FileIdService",
            "file" : "ids"
        }
    },
    "storage":{
        "store" : "jip.jobs.FileJobStore",
        "directory" : "jobs"
    }
}